<div class="container">
	<div class="row">
		<div>
			<h3>Cases where identified needs could not be met and reasons </h3>
			<div id = "validation_div"></div>
			<%= form_tag("/all_reports/cases_where_identified/submit_form", class: "form-inline" ,method: "post", class: 'form-inline', name:"myForm", id:"submit_form_for_cases_where_identified") do %>
				<label>Start Date</label>
				<%= date_field_tag(:start_date, Date.today, :id => 'start_date_field', :max => Date.today, :required => true)%>

				<label>End Date</label>
				<%= date_field_tag(:end_date, Date.today, :id => 'end_date_field', :max => Date.today, :required => true)%>

				<button type="submit">Submit</button>
			<% end %>
		</div>
	</div>
</div>
