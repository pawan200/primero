<div class="container">
	<div class="row">
		<h3>Cases where identified needs could not be met and reasons </h3>
		<div class="large-12 small-12 columns">
			<table class="dataTable in_content_area reports">
				<thead>
					<tr>
							<td>Reasons for inability to meet a need</td>
							<td>Emotional/ Psychological</td>
							<td>Shelter/ Protection</td>
							<td>Educational</td>
							<td>Financial</td>
							<td>Paralegal</td>
							<td>Familial</td>
					</tr>
				</thead>
				<tbody>
					<tr>
						<td>Child restored to family and ran away</td>
						<td>2</td>
						<td>1</td>
						<td>-</td>
						<td>-</td>
						<td>-</td>
						<td>-</td>
					</tr>
					
				
				
				<% @temp.each do |x| %>
					<tr>
						<td id="type_of_reason"><%= x[:type_of_reason] %></td>
						<td id="emotional_psychological_count"><%= x[:values][:emotional_psychological_count] %></td>
						<td id="shelterprotection_needs_count"><%= x[:values][:shelterprotection_needs_count] %></td>
						<td id="educational_vocational_count"><%= x[:values][:educational_vocational_count] %></td>
						<td id="financial_needs_count"><%= x[:values][:financial_needs_count] %></td>
						<td id="paralegal_needs_count"><%= x[:values][:paralegal_needs_count] %></td>
						<td id="familial_needs_count"><%= x[:values][:familial_needs_count] %></td>
					</tr>
				<% end %>
				
				
				</tbody>	
			</table>
		</div>
	</div>
</div>
